programa {
  funcao inicio() {
    real angulo1
    real angulo2
    real angulo3

    escreva("Digite o primeiro ângulo do triângulo (graus): ")
      leia(angulo1)

        escreva("Digite o segundo ângulo do triângulo (graus): ")
          leia(angulo2)

             angulo3 = 180 - (angulo1 + angulo2)

                escreva("O terceiro ângulo do triângulo é: ", angulo3 , " graus")

  }
}
